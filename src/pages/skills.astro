---
import SkillsList from "../components/SkillsList.astro";
import Default from "../layouts/Default.astro";
import template from "../template";

const { skills } = template;
---

<Default title={skills.title}>
  {
    skills.content.map((skill) => (
      <SkillsList title={skill.title} content={skill.content} />
    ))
  }
</Default>
